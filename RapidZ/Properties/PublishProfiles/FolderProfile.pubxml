<?xml version="1.0" encoding="utf-8"?>
<Project>
  <!-- 
    This file contains publishing properties for Windows and macOS applications.
    Include this file in your publish workflow to properly configure icons.
  -->
  <PropertyGroup>
    <!-- Windows-specific settings -->
    <ApplicationIcon>Assets\Images\favicon.ico</ApplicationIcon>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    
    <!-- macOS-specific settings -->
    <CFBundleName>RapidZ</CFBundleName>
    <CFBundleDisplayName>RapidZ</CFBundleDisplayName>
    <CFBundleIdentifier>com.yourcompany.rapidz</CFBundleIdentifier>
    <CFBundleVersion>1.0.0</CFBundleVersion>
    <CFBundleShortVersionString>1.0.0</CFBundleShortVersionString>
    <CFBundlePackageType>APPL</CFBundlePackageType>
    <CFBundleSignature>????</CFBundleSignature>
    <CFBundleExecutable>RapidZ</CFBundleExecutable>
    <CFBundleIconFile>favicon.icns</CFBundleIconFile>
    <NSPrincipalClass>NSApplication</NSPrincipalClass>
    <NSHighResolutionCapable>true</NSHighResolutionCapable>
  </PropertyGroup>
</Project>
